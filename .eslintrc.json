{
  "extends": "xo/esnext",
  "plugins": [
    "security"
  ],
  "rules": {
    "padded-blocks": ["off"],
    "operator-assignment": ["off"],
    "eqeqeq": ["error", "allow-null"],
    "no-eq-null": ["off"],
    "quote-props": ["error", "as-needed"],
    "capitalized-comments": ["warn"],
    "curly": ["error", "multi-line"],
    "arrow-parens": ["off"],
    "no-debugger": ["warn"]
  }
}
