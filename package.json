{
  "name": "baumeister",
  "title": "Baumeister",
  "description": "The frontend build workflow for your needs",
  "homepage": "https://github.com/micromata/baumeister",
  "author": {
    "name": "Michael KÃ¼hnel",
    "email": "m.kuehnel@micromata.de",
    "url": "http://www.micromata.de"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "license": "MIT",
  "version": "2.0.1",
  "repository": "micromata/baumeister",
  "private": true,
  "scripts": {
    "clean": "del dist",
    "clean:dev": "del server",
    "pretest": "gulp lint --production",
    "test": "gulp test --production",
    "test:watch": "gulp test --watch",
    "start": "npm-run-all clean:dev handlebars webpack:server",
    "dev": "gulp build",
    "serve": "gulp serve",
    "watch": "npm-run-all clean:dev handlebars webpack:watch",
    "lint": "gulp lint --production",
    "lint:fix": "gulp lint",
    "build": "npm-run-all clean webpack",
    "build:dev": "npm-run-all clean:dev webpack:dev",
    "build:check": "gulp serve --production",
    "release:patch": "gulp release --bump patch",
    "release:minor": "gulp release --bump minor",
    "release:major": "gulp release --bump major",
    "postmerge": "npm install",
    "webpack": "cross-env NODE_ENV=production webpack --config build/webpack.prod.babel.js --hide-modules --progress",
    "webpack:dev": "cross-env NODE_ENV=development webpack --config build/webpack.dev.babel.js --hide-modules --progress",
    "webpack:watch": "cross-env NODE_ENV=development webpack --config build/webpack.dev.babel.js --hide-modules --watch",
    "webpack:server": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.babel.js --open",
    "handlebars": "babel-node build/handlebars.js"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-beta.3",
    "jquery": "3.2.1",
    "popper.js": "^1.12.9"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.4",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "browser-sync": "^2.23.2",
    "chalk": "^2.3.0",
    "copy-webpack-plugin": "^4.3.1",
    "core-js": "^2.5.3",
    "cross-env": "^5.1.3",
    "cssnano": "^3.10.0",
    "del-cli": "^1.1.0",
    "eslint-config-xo": "0.19.0",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-security": "^1.4.0",
    "globby": "^7.1.1",
    "gulp": "^4.0.0",
    "gulp-banner": "^0.1.3",
    "gulp-bootlint": "^0.8.1",
    "gulp-bump": "^3.0.0",
    "gulp-changed": "^3.2.0",
    "gulp-concat": "^2.6.1",
    "gulp-conventional-changelog": "^1.1.7",
    "gulp-eslint": "^4.0.0",
    "gulp-front-matter": "^1.3.0",
    "gulp-git": "^2.5.0",
    "gulp-html": "^0.4.7",
    "gulp-htmlmin": "^4.0.0",
    "gulp-imagemin": "^4.1.0",
    "gulp-notify": "^3.1.0",
    "gulp-nsp": "^2.4.2",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^7.0.1",
    "gulp-processhtml": "^1.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-rev-append": "^0.1.8",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.3",
    "gulp-stylelint": "^6.0.0",
    "gulp-touch": "^1.0.1",
    "gulp-util": "^3.0.8",
    "gulpsmith": "^0.6.0",
    "husky": "^0.14.3",
    "jest-cli": "^22.0.4",
    "jstransformer-handlebars": "^1.0.0",
    "log-symbols": "^2.1.0",
    "merge-stream": "^1.0.1",
    "metalsmith": "^2.3.0",
    "metalsmith-filter": "^1.0.2",
    "metalsmith-in-place": "^4.0.0",
    "metalsmith-layouts": "^1.8.1",
    "metalsmith-register-helpers": "^0.4.0",
    "minimist": "^1.2.0",
    "npm-run-all": "^4.1.2",
    "perfy": "^1.1.2",
    "regenerator": "^0.12.3",
    "semver": "^5.4.1",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "uglifyjs-webpack-plugin": "^1.1.5",
    "uncss": "^0.16.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/gulp/",
      "<rootDir>/node_modules/"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/app/**/*.{js,jsx}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "text-summary"
    ]
  }
}
